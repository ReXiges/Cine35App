<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cine35app.esteb.cine35app">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AdmiActivity" />
        <activity android:name=".ClientActivity" />
        <activity android:name=".SignUpActivity" />
        <activity android:name=".InsertMovieActivity" />
        <activity android:name=".UserManagmentActivity" />
        <activity
            android:name=".ComentsActivity"
            android:label="@string/title_activity_coments"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".peliculaActivity"
            android:label="@string/title_activity_pelicula"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MovieEditor"
            android:label="@string/title_activity_movie_editor"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>